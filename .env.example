# ─── Database ───
DATABASE_URL="postgresql://postgres:password@localhost:5432/fio?schema=public"

# ─── Auth (NextAuth) ───
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="generate-a-secret-with-openssl-rand-base64-32"

# ─── WebSocket Server ───
WS_SERVER_PORT=8080
WS_SERVER_URL="ws://localhost:8080"

# ─── Agent API ───
AGENT_API_SECRET="generate-a-secret-for-agent-key-signing"

# ─── Supabase (optional, for hosted Postgres) ───
# SUPABASE_URL="https://your-project.supabase.co"
# SUPABASE_ANON_KEY="your-anon-key"
# SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# ─── Rate Limits ───
RATE_LIMIT_AGENT_RPM=120
RATE_LIMIT_USER_RPM=60
RATE_LIMIT_IP_RPM=30
